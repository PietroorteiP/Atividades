programa
{
	
inclua biblioteca Matematica --> m
	funcao inicio()
	{
	inteiro valA, valB, maior, valC, valD, maior2, maior3
	
		escreva("Me de um número \n")
		leia(valA)
		escreva("Me de outro valor \n")
		leia(valB)
    escreva("Me da mais um número \n")
    leia(valC)
    escreva("Me da mais algum \n")
    leia(valD)

 
		maior = m.maior_numero(valA, valB)
    maior2 = m.maior_numero(valC, valD)
    maior3 = m.maior_numero(maior, maior2)
		escreva("O maior número é ", maior3)
	}
}

